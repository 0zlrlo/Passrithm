<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/pwlist_item_background"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rectangle"
        />

    <TextView
        android:id="@+id/algolist_item_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@id/pwlist_item_background"
        android:layout_alignBottom="@id/pwlist_item_background"
        android:layout_alignTop="@id/pwlist_item_background"
        android:text="text"
        android:gravity="center"
        android:layout_marginStart="20dp"
        />


    <ImageButton
        android:id="@+id/button"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="0dp"
        android:src="@drawable/ic_password_share"
        android:adjustViewBounds="true"/>

</RelativeLayout>